{
  "manifest_version": 3,
  "name": "PIU Web Extension",
  "description": "QOL update on PIU webpage",
  "version": "1.0.1",
  "content_scripts": [
    {
        "js": [
          "scripts/content.js"
        ],
        "matches": [
            "https://phoenix.piugame.com/my_page/*",
            "https://piugame.com/my_page/*"
        ]
    }
  ],
  "permissions": [
    "storage"
  ]
}